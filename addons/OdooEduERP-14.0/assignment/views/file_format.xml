<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE html>

<odoo>
    <!-- Tree View Of Teacher Assignment Information -->
    <record id="view_file_format_tree" model="ir.ui.view">
        <field name="name">school.file.format.tree</field>
        <field name="model">file.format</field>
        <field name="arch" type="xml">
            <tree string="File format">
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="view_file_format_form" model="ir.ui.view">
        <field name="name">school.file.format.form</field>
        <field name="model">file.format</field>
        <field name="arch" type="xml">
            <form string="File format">
                <field name="name" />
            </form>
        </field>
    </record>
</odoo>
